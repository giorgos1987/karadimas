<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Cart.
    -->
    <changeSet id="20240224182139-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="cart_id"
                                 baseTableName="rel_cart__job"
                                 constraintName="fk_rel_cart__job__cart_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cart"/>

        <addForeignKeyConstraint baseColumnNames="job_id"
                                 baseTableName="rel_cart__job"
                                 constraintName="fk_rel_cart__job__job_id"
                                 referencedColumnNames="id"
                                 referencedTableName="job"/>

        <addForeignKeyConstraint baseColumnNames="customer_details_id"
                                 baseTableName="cart"
                                 constraintName="fk_cart__customer_details_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer_details"/>
    </changeSet>
</databaseChangeLog>
